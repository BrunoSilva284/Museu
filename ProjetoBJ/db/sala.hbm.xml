 <?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="model.sala" table="sala">
       <id name="idSala" column="idSala" type="integer">
         <generator class="native"></generator>
       </id>
       <property name="numSala" column="numSala" type="integer"></property>
	   <property name="lotacaoSala" column="lotacaoSala" type="integer"></property>
    </class>
    </hibernate-mapping>